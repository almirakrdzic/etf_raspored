@model IEnumerable<DataLayer.book>

@{
    ViewBag.Title = "Delete";
}

     <div class="span12"></div>
 @foreach (var item in Model) {    
<div class="span3">
<a id="@item.id"  href="javascript:getBook(@item.id)" class="thumbnail">
<img data-src="holder.js/260x180" alt="260x180" src="~/Content/img/book.png" /></a>
 <div class="caption">
<h3 style="text-align:center">@Html.DisplayFor(modelItem => item.title)</h3>                   
 </div> 
</div>  
}    
